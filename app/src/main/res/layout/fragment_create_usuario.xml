<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.usuario.create_usuario"
    android:padding="16dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Crear Usuario"
            android:textColor="#000000"
            android:textSize="20sp" />

        <EditText
            android:id="@+id/text_create_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="ID Usuario"
            android:inputType="text" />

        <EditText
            android:id="@+id/text_create_nombre"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Nombre"
            android:inputType="text" />

        <EditText
            android:id="@+id/text_create_correo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Correo Electronico"
            android:inputType="text" />

        <EditText
            android:id="@+id/text_create_contra"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="ContraseÃ±a"
            android:inputType="text" />

        <EditText
            android:id="@+id/text_create_ubi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="ID Ubicacion"
            android:inputType="text" />

        <Button
            android:id="@+id/btn_create_usu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Crear" />
    </LinearLayout>
</FrameLayout>